{
	"name": "Jobs",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "concurrently -n \"scraper,processing,bot\" \"pnpm dev:scraper\" \"pnpm dev:processing\" \"pnpm dev:bot\"",
		"dev:scraped": "concurrently -n \"processing,storage,bot\" \"pnpm dev:processing\" \"pnpm dev:storage\" \"pnpm dev:bot\"",
		"dev:scraper": "nodemon --watch scraper/index.ts --exec ts-node scraper/index.ts",
		"dev:processing": "nodemon --watch post-processing/index.ts --exec ts-node post-processing/index.ts",
		"dev:storage": "nodemon --watch storage/index.ts --exec ts-node storage/index.ts",
		"dev:bot": "nodemon --watch discord-bot --exec ts-node discord-bot/index.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.13.1",
	"dependencies": {
		"amqplib": "^0.10.8",
		"discord.js": "^14.21.0",
		"jsonrepair": "^3.13.0",
		"mongoose": "^8.16.5",
		"ollama": "^0.5.16",
		"pm2": "^6.0.8",
		"puppeteer": "^24.15.0"
	},
	"devDependencies": {
		"@types/amqplib": "^0.10.7",
		"concurrently": "^9.2.0",
		"nodemon": "^3.1.10",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	}
}
